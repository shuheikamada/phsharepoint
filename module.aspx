<script>
$('nav').hover(function() {
	//マウスを乗せたら色が変わる
	$('.overlay').addClass('disp');
  }, function() {
	    //色指定を空欄にすれば元の色に戻る
	$('.overlay').removeClass('disp');
});

$('nav .box ul li').hover(function() {
	$(this).addClass('active');
},
function () {
	$(this).removeClass('active');
});


/* gotop */
$(function() {
	// hide #back-top first
	// fade in #back-top
			var rightnav = $('.rightnav').offset().top - 1145;
	$('.rightnav').hide();
			$(window).scroll(function () {
					if ($(this).scrollTop() > 100) {
							$('.rightnav').fadeIn();
					} else {
							$('.rightnav').stop(true, true).fadeOut();
					}
				if($(window).scrollTop() > rightnav){
					$('.rightnav').removeClass('fixed');

				}else{
					$('.rightnav').addClass('fixed');
				}
			});
			// scroll body to 0px on click
			$('.gotop').click(function () {
					$('body,html').animate({
							scrollTop: 0
					}, 800);
					return false;
			});
});

$('.sp-menu-btn').click(function(){
	$('header').toggleClass('active');
});

$('header .parent').click(function(){
	$(this).toggleClass('open');
});

</script>
